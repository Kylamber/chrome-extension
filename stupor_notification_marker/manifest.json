{
  "name": "Stupor Notification Marker",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Marks notification (pengumuman) with read or highlight it",
  "permissions": [
    "storage"
  ],
  "content_scripts":[
    {
      "matches": ["https://studentportal.unpar.ac.id/*"],
      "js": ["content.js"]
  }
],
  "background": {
    "service_worker": "content.js"
  }
}
